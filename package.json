    "dependencies": {
        "@types/lodash": "^4.14.149",
        "ajv": "^6.12.0",
        "body-parser": "^1.19.0",
        "chalk": "^3.0.0",
        "clipboardy": "^2.2.0",
        "csv-writer": "^1.6.0",
        "date-fns": "^2.10.0",
        "express": "^4.17.1",
        "googleapis": "47.0.0",
        "io-ts": "^2.1.2",
        "lodash": "^4.17.15",
        "open": "^7.0.2",
        "p-each-series": "^2.1.0",
        "p-map-series": "^2.1.0",
        "plaid": "^4.7.0",
        "prompts": "^2.3.1",
        "ts-node": "^8.6.2",
        "typescript": "^3.8.3",
        "typescript-json-schema": "^0.42.0",
        "yargs": "^15.1.0"
